{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A boilerplate of things that shouldn't exist",
  "main": "index.js",
  "scripts": {
    "webpack-babel": "babel-node ./node_modules/webpack/bin/webpack",
    "webpack-dev-server-babel": "babel-node ./node_modules/webpack-dev-server/bin/webpack-dev-server",
    "babel-node": "babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config ./webpack/webpack.dev.babel.js -d --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config ./webpack/webpack.prod.babel.js",
    "build_server": "babel server --ignore dist -s -D -d ./server/dist",
    "start_server": "nodemon --exec npm run babel-node -- ./server/server.js"
  },
  "license": "MIT",
